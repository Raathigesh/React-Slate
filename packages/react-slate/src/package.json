{
  "name": "reactor",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config ./editor/webpack.config.js",
    "clean": "rimraf dist/",
    "start-dash": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server -d --inline --hot --progress --no-info --quiet",
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info --config ./editor/webpack.config.js",
    "typings": "rimraf typings/ && typings install",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cover": "jest --coverage",
    "commit": "git-cz"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.zone24x7.lk:KTENG/Reactor.git"
  },
  "bugs": {
    "url": "https://git.zone24x7.lk/KTENG/Reactor/issues"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "minimist": "^1.2.0",
    "npm-programmatic": "0.0.6",
    "npmi": "^2.0.1",
    "server-destroy": "^1.0.1",
    "socket.io": "^1.7.2",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.4.0",
    "@types/node": "^6.0.46",
    "@types/react": "^0.14.47",
    "@types/react-addons-css-transition-group": "^0.14.17",
    "@types/react-addons-transition-group": "^0.14.17",
    "@types/react-dom": "^0.14.18",
    "ast-traverse": "^0.1.1",
    "autoprefixer": "^6.3.3",
    "awesome-typescript-loader": "^1.0.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "brace": "^0.9.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.20.2",
    "commitizen": "^2.8.6",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.4.0",
    "eslint": "^3.2.0",
    "esprima": "^3.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flexboxgrid": "^6.3.1",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "jest-cli": "^16.0.1",
    "json-loader": "^0.5.4",
    "mobx": "^2.6.3",
    "mobx-react": "^4.0.3",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-ace": "^4.1.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.3.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.0",
    "react-flexbox-grid": "^0.10.2",
    "react-router": "^2.5.2",
    "recast": "^0.11.17",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "socket.io-client": "^1.5.1",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "stylelint": "^6.8.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "superagent": "^3.3.1",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "tslint-microsoft-contrib": "^2.0.12",
    "tslint-react": "^1.0.0",
    "typescript": "2.0.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-split-by-path": "0.0.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/test/preprocessor.js",
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "^.+\\.(css|less)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
